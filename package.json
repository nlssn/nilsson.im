{
  "name": "akk",
  "version": "1.0.0",
  "description": "Website for allakankoda.se",
  "main": "index.html",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/assets/styles:public/assets/styles",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass --no-source-map src/assets/styles:public/assets/styles",
    "build:eleventy": "eleventy",
    "postbuild": "postcss public/assets/styles/*.css -u autoprefixer cssnano -r --no-map",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass build:eleventy postbuild"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "sass": "^1.43.4"
  },
  "browserslist": [
    "> 0.5%",
    "not IE 11"
  ]
}
